{
  "name": "score-plus-back",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "AWS_PROFILE=score-plus-sdx sls offline",
    "debug": "AWS_PROFILE=score-plus-sdx sls offline",
    "debug:datavalid-pf-basic": "AWS_PROFILE=score-plus-prd AWS_REGION=us-east-1 sls invoke local --function worker-datavalid-pf-basic --path local/debug/worker-datavalid-pf-basic.json",
    "debug:datavalid-pf-facial": "AWS_PROFILE=score-plus-prd AWS_REGION=us-east-1 sls invoke local --function worker-datavalid-pf-facial --path local/debug/worker-datavalid-pf-facial.json",
    "debug:datavalid-pf-facial-cdv": "AWS_PROFILE=score-plus-prd AWS_REGION=us-east-1 sls invoke local --function worker-datavalid-pf-facial-cdv --path local/debug/worker-datavalid-pf-facial-cdv.json",
    "test": "LOGGER_LEVEL=silent jest",
    "lint": "tsc --noEmit && eslint --ext .ts src",
    "clean": "rimraf dist",
    "prune": "npm prune --production",
    "tsc": "tsc -p tsconfig.build.json",
    "tsc-alias": "tsc-alias -p tsconfig.build.json",
    "mv:node_modules": "mv node_modules dist/node_modules",
    "cp:templates": "cp -r src/templates dist/src",
    "cp:assets": "cp -r src/assets dist/src",
    "zip": "cd dist && zip -r latest.zip ./",
    "build": "run-s clean tsc prune mv:node_modules zip",
    "aws-lambda": "AWS_PROFILE=score-plus-sdx AWS_REGION=us-east-1 aws lambda update-function-code --function-name teste --zip-file fileb://dist/scoreplus-back.zip"
  },
  "repository": {
    "type": "git",
    "url": "git@git-pessoal:Kazu-Kadota/scoreplus-back.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.504.0",
    "@aws-sdk/client-s3": "^3.525.0",
    "@aws-sdk/client-ses": "^3.504.0",
    "@aws-sdk/client-sns": "^3.504.0",
    "@aws-sdk/client-sqs": "^3.504.0",
    "@aws-sdk/lib-dynamodb": "^3.504.0",
    "@aws-sdk/util-dynamodb": "^3.504.0",
    "@smithy/types": "^2.10.1",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "csv-stringify": "^6.4.0",
    "dayjs": "^1.11.10",
    "joi": "~17.12.1",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^10.2.0",
    "mustache": "^4.2.0",
    "parse-multipart-data": "^1.5.0",
    "pdfjs": "^2.5.3",
    "pino": "^8.18.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.23.3",
    "@types/aws-lambda": "^8.10.133",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mustache": "^4.2.5",
    "@types/node": "^20.11.16",
    "@types/serverless": "^3.12.22",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "prune": "^0.0.2",
    "rimraf": "^5.0.5",
    "serverless": "^3.38.0",
    "serverless-esbuild": "^1.51.0",
    "serverless-offline": "^13.3.3",
    "serverless-offline-local-authorizers-plugin": "^1.2.0",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3"
  }
}
